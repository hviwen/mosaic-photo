{
  "name": "mosaic-photo",
  "version": "2.0.0",
  "description": "A modern photo mosaic tool built with Vue 3 + Vite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "pnpm -s typecheck && pnpm -s build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "pnpm -s lint --fix"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^10.11.1",
    "pinia": "^2.3.1",
    "vue": "^3.5.27",
    "vuetify": "3.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.20",
    "code-inspector-plugin": "^1.4.0",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.38",
    "sass": "^1.97.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.21",
    "vue-eslint-parser": "^9.0.0",
    "vue-tsc": "^2.2.12"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@parcel/watcher"
    ]
  },
  "homepage": "https://github.com/hviwen/mosaic-photo#readme"
}